<div class="row">
    <div class="span4">

  <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "well"}) do |f| %>
    <%= devise_error_messages! %>

  <fieldset>
      <legend>Sign In</legend>

    <div><%= f.input :first_name %><br /></div>

        <div><%= f.input :last_name %><br /></div>

        <div><%= f.input :profile_name %><br /></div>


    <div class="field">
      <%= f.input :email %><br />
    </div>

    <div class="field">
      <%= f.input :password %>
      <% if @validatable %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
    </div>

    <div class="field">
      <%= f.input :password_confirmation %><br />
     
    </div>

    <div class="actions">
      <%= f.button :submit, "Sign up" %>
    </div>
  </fieldset>
  <% end %>

  <%= render "devise/shared/links" %>

  </div>
  <div class="span8">
   <h3>Nigga, you gotta Register before you do shiet.</h3>
   </div>
</div>